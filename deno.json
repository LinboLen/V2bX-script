{
  "imports": {
    "$": "https://deno.land/x/dax@0.39.2/mod.ts",
    "@david/dax": "jsr:@david/dax@^0.44.1",
    "@types/node": "npm:@types/node@^24.2.0"
  },
  "tasks": {
    "debug:initconfig": "deno run -A initconfig.ts",
    "debug:install": "deno run -A install.ts",
    "debug:v2bx": "deno run -A V2bX.ts",
    "bundle:initconfig": "deno bundle -o dist/initconfig.ts initconfig.ts",
    "bundle:install": "deno bundle -o dist/install.ts install.ts",
    "bundle:v2bx": "deno bundle -o dist/v2bx.ts V2bX.ts",
    "bundle": "deno task bundle:initconfig && deno task bundle:install && deno task bundle:v2bx",
    "build:install:linux-x64": "deno compile -A --target x86_64-unknown-linux-gnu --output dist/install-linux-x64 install.ts",
    "build:install:linux-arm64": "deno compile -A --target aarch64-unknown-linux-gnu --output dist/install-linux-arm64 install.ts",
    "build:v2bx:linux-x64": "deno compile -A --target x86_64-unknown-linux-gnu --output dist/v2bx-linux-x64 V2bX.ts",
    "build:v2bx:linux-arm64": "deno compile -A --target aarch64-unknown-linux-gnu --output dist/v2bx-linux-arm64 V2bX.ts",
    "build:initconfig:linux-x64": "deno compile -A --target x86_64-unknown-linux-gnu --output dist/initconfig-linux-x64 initconfig.ts",
    "build:initconfig:linux-arm64": "deno compile -A --target aarch64-unknown-linux-gnu --output dist/initconfig-linux-arm64 initconfig.ts",
    "build": "deno task build:install:linux-x64 && deno task build:install:linux-arm64 && deno task build:v2bx:linux-x64 && deno task build:v2bx:linux-arm64 && deno task build:initconfig:linux-x64 && deno task build:initconfig:linux-arm64"
  }
}
